    <mapping resource="entity/Ubicacion.hbm.xml"/>
    <mapping resource="entity/Estado.hbm.xml"/>
    <mapping resource="entity/Sala.hbm.xml"/>
    <mapping resource="entity/TipoMedio.hbm.xml"/>
    <mapping resource="entity/Domicilio.hbm.xml"/>
    <mapping resource="entity/Edicion.hbm.xml"/>
    <mapping resource="entity/Documento.hbm.xml"/>
    <mapping resource="entity/Region.hbm.xml"/>
    <mapping resource="entity/Tipo.hbm.xml"/>
    <mapping resource="entity/Rol.hbm.xml"/>
    <mapping resource="entity/Reserva.hbm.xml"/>
